    <!-- http://www.semanticweb.org/luke/ontologies/2019/11/hierarquia_uniao#Orgaos_Independentes -->

    <owl:Class rdf:about="http://www.semanticweb.org/luke/ontologies/2019/11/hierarquia_uniao#Orgaos_Independentes">
